{
    "version": 2,
    "builds": [
      {
        "src": "api/getAudioSource.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/getAudioSource",
        "dest": "/api/getAudioSource.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }